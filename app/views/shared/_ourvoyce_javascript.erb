<script type="text/javascript">
	var state = "";
  var model = "ourvoyce";
	var curID = "";
	
	function setState(st){
	  state = st;
	  sendToGraph(model + "/age/" + state);
	  
	  $(".vote-table tbody tr").removeClass("selected")
	  $(".vote-table tbody tr." + state).addClass("selected");
	}
	
	function sendToMap(dataString) {
    thisMovie("home-map").sendTextToFlash(dataString);
  }
  
  function sendToGraph(dataString) {
    thisMovie("bar-graph").sendTextToFlash(dataString);
  }
	
	function thisMovie(movieName) {
    if (navigator.appName.indexOf("Microsoft") != -1) {
      return window[movieName];
    } else {
      return document[movieName];
    }
  }
	
	var mapvars = {
	  xmlpath: model + "/map/",
	  baseURL:"/services/website/"
	};
	
	var agevars = {
	  xmlpath: model + "/age/",
	  baseURL:"/services/website/",
	  barwidth: 25
	};
	
  var params = {
      menu: "false",
      wmode: "transparent"
  };
  
  var attributes = {
    allowfullscreen: "true",
    allowscriptaccess: "always",
    wmode: "transparent"
  };
  
  //swfobject.embedSWF("/swf/map.swf", "home-map", "567", "350","10.0.0", "", mapvars, params, attributes);
  //swfobject.embedSWF("/swf/bar-graph.swf", "bar-graph", "350", "200","10.0.0", "", agevars, params, attributes);
  
  swfobject.embedSWF("/swf/map.swf", "home-map", "350", "270","10.0.0", "", mapvars, params, attributes);
  swfobject.embedSWF("/swf/bar-graph.swf", "bar-graph", "350", "240","10.0.0", "", agevars, params, attributes);     
  
</script>