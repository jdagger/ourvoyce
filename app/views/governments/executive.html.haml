= render :partial => "shared/government_menu"
%table
  %thead
    %tr
      %th= table_sort_header_link :header => "Vote", :sort_column => 'votedate', :link_params => { :filter => params[:filter], :page => params[:page] }
      %th Picture
      %th= table_sort_header_link :header => "Name", :sort_column => 'name', :link_params => { :filter => params[:filter], :page => params[:page] }
      %th= table_sort_header_link :header => "Title", :sort_column => 'default', :link_params => { :filter => params[:filter], :page => params[:page] }
      %th= table_sort_header_link :header => "SS", :sort_column => 'social', :link_params => { :filter => params[:filter], :page => params[:page] }
      %th= table_sort_header_link :header => "PR", :sort_column => 'participation', :link_params => { :filter => params[:filter], :page => params[:page] }
    %tbody
    - @presenter.executives.each do |executive|
      %tr
        %td= render :partial => 'shared/vote_control', :object =>{:id => executive.id, :support_type => executive.support_type, :post_url => :government_vote}, :as => :d
        %td
          %img{:src => get_government_image_64(executive.logo, executive.government_type_id), :alt => 'logo'}
        %td= "#{executive.first_name} #{executive.last_name}"
        %td= executive.title
        %td= executive.social_score
        %td= executive.participation_rate
