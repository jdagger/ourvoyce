.left-content
  %h1 ourvoYce
  %div.vote-table-wrap
    %table.vote-table
      %thead
        %tr
          %th State
          %th # of Users
          %th % of Population
      %tbody
        - @states.each do |state|
          %tr
            %td= state.name
            %td= state.user_count
            %td= number_with_precision(100 * state.user_count.to_f / state.population, :precision => 2).to_s + '%'
  %div.vote-table-cap
.graph-contents
  = render :partial => "shared/map_graph", :locals => {:id => nil, :model => "ourvoyce", :title => ""}
